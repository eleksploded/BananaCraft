package com.pikachu923.bananacraft.CreativeTab;

import com.pikachu923.bananacraft.reference.Reference;

import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.item.Item;

public class GemTab {
public static final CreativeTabs GemTab = new CreativeTabs(Reference.MOD_ID + " Gems"){
	@Override
	public Item getTabIconItem(){
		return Gems.UltraGem;
	}
	public String getTranslatedTabLable(){
		return "BananaCraft Gems";
	}
};
}
