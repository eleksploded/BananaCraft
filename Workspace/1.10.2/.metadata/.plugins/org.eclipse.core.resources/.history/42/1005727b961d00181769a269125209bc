package com.pikachu923.BananaCraft.Items3;

import com.pikachu923.BananaCraft.init.Tier3Tools;
import com.pikachu923.BananaCraft.reference.Reference;

import net.minecraft.item.ItemHoe;

public class CorruptHoe extends ItemHoe {
	private String Texture;

	public CorruptHoe(String unlocalName) {
        super(Tier3Tools.CorruptBanana);
        this.setUnlocalizedName(Reference.RESOURCE_PREFIX + unlocalName);
        this.Texture = Reference.RESOURCE_PREFIX + unlocalName;
        this.setCreativeTab(Reference.CreativeTab);
    }

}
