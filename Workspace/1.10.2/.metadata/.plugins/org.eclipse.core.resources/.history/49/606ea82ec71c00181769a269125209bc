package com.pikachu923.BananaCraft.init;

import com.pikachu923.BananaCraft.Items3.CorruptAxe;
import com.pikachu923.BananaCraft.Items3.CorruptHoe;
import com.pikachu923.BananaCraft.Items3.CorruptPickaxe;
import com.pikachu923.BananaCraft.Items3.CorruptShovel;
import com.pikachu923.BananaCraft.Items3.CorruptSword;

import net.minecraft.item.Item;
import net.minecraft.item.ItemAxe;
import net.minecraft.item.ItemHoe;
import net.minecraft.item.ItemPickaxe;
import net.minecraft.item.ItemSpade;
import net.minecraft.item.ItemSword;
import net.minecraftforge.common.util.EnumHelper;

public class Tier3Tools {
	public static Item.ToolMaterial CorruptBanana = EnumHelper.addToolMaterial("CorruptBanana", 6, 3000, 15.0F, 7, 20);
	public static ItemAxe CorruptAxe = new CorruptAxe("CorruptAxe");
	public static ItemPickaxe CorruptPickaxe = new CorruptPickaxe("CorruptPickaxe");
	public static ItemSpade CorruptShovel = new CorruptShovel("CorruptShovel");
	public static ItemSword CorruptSword = new CorruptSword("CorruptSword");
	public static ItemHoe CorruptHoe = new CorruptHoe("CorruptHoe");
	
	public static void Init() {
		func.registerItem(CorruptAxe, "CorruptAxe");
		func.registerItem(CorruptPickaxe, "CorruptPickaxe");
		func.registerItem(CorruptShovel, "CorruptShovel");
		func.registerItem(CorruptSword, "CorruptSword");
		func.registerItem(CorruptHoe, "CorruptHoe");
	}
}
